{
    "name": "aerni/paparazzi",
    "description": "Generate images of your entries and terms",
    "keywords": [
        "statamic"
    ],
    "homepage": "https://github.com/aerni/statamic-paparazzi",
    "license": "proprietary",
    "authors": [
        {
            "name": "Michael Aerni",
            "email": "hello@michaelaerni.ch",
            "homepage": "https://michaelaerni.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0",
        "laravel/prompts": "*",
        "spatie/browsershot": "^4.0",
        "spatie/laravel-ray": "^1.32",
        "statamic/cms": "^4.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.1 || ^7.0",
        "orchestra/testbench": "^7.1 || ^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Aerni\\Paparazzi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aerni\\Paparazzi\\Tests\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    },
    "extra": {
        "statamic": {
            "name": "Paparazzi",
            "description": "Generate images of your entries and terms"
        },
        "laravel": {
            "providers": [
                "Aerni\\Paparazzi\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
